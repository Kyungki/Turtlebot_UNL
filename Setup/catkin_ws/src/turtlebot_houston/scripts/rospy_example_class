"""
Run the rospy_example_class node
"""

from turtlebot_houston.rospy_example_class import example_class


def _main():
    node = example_class()
    try:
        node.run()
    except Exception, e:
        print "Exception excepted!\r\n{}".format(e)

if __name__ == "__main__":
    _main()
